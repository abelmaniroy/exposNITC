int main(){
	decl
	int t,desc,a;
	str file;
	str word;
	enddecl
	begin
		t=exposcall("Write",-2,"--FILE NAME--");
		t=exposcall("Read",-1,file);
		desc=exposcall("Open",file);
		if(desc>=0) then
			while(1==1) do
				a=exposcall("Read",desc,word);
				if(a==-2) then
					break;
				endif;
				t=exposcall("Write",-2,word);
				
			endwhile;
		endif;
		desc=exposcall("Close",file);
		return 0;
	end
}
