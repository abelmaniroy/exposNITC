int main(){
	decl
	int t,m,pid;
	
	enddecl
	begin
		pid = exposcall("Fork");
		if(pid!=0) then
			t=exposcall("Wait",pid);
		endif;
		pid = exposcall("Fork");
		if(pid!=0) then
			t=exposcall("Wait",pid);
		endif;
		pid = exposcall("Fork");
		if(pid!=0) then
			t=exposcall("Wait",pid);
		endif;
					
		t=exposcall("Exec","pid.xsm");
		
		
		
		
		return 0;
	end
}
